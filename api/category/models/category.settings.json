{
  "connection": "default",
  "collectionName": "categories",
  "info": {
    "name": "category"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "hidden": {
      "type": "boolean"
    },
    "isContent": {
      "type": "boolean"
    },
    "isSinglePage": {
      "type": "boolean"
    },
    "blog_posts": {
      "collection": "blog-post",
      "via": "categories"
    },
    "order": {
      "type": "integer"
    }
  }
}
