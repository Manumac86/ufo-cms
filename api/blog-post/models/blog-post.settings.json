{
  "connection": "default",
  "collectionName": "blog_posts",
  "info": {
    "name": "blog-post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Content": {
      "type": "richtext"
    },
    "Date": {
      "type": "datetime"
    },
    "Pictures": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "categories": {
      "collection": "category",
      "via": "blog_posts",
      "dominant": true
    }
  }
}
